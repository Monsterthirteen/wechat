
<!-- swiper轮播 -->
<view>
  <swiper
    indicator-dots='{{indicatorDots}}'
    indicator-color='{{indicatorColor}}'
    indicator-active-color='{{indivatorActiveColor}}'
    autoplay='{{autoplay}}'
    interval='{{interval}}'
    duration='{{duration}}'
  >
    <block wx:for="{{imgUrls}}">
      <swiper-item>
        <image src='{{item}}' class='slide-images'></image>
      </swiper-item>
    </block>
  </swiper>
</view>
<slot></slot>


    
